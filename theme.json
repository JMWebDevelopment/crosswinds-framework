				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--max-60)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--max-48)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--max-36)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "var(--wp--custom--font-weight--semi-bold)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "30px",
			"padding": {
				"bottom": "0px",
				"left": "30px",
				"right": "30px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--regular)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "sidebar",
			"name": "sidebar",
			"title": "Sidebar"
		},
		{
			"area": "uncategorized",
