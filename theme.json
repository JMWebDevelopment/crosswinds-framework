				{
					"name": "Black and White",
					"slug": "black-and-white",
					"colors": [ "#000000", "#ffffff" ]
				},
				{
					"name": "Primary and White",
					"slug": "primary-and-white",
					"colors": [ "#343A40", "#ffffff" ]
				},
				{
					"name": "Secondary and White",
					"slug": "secondary-and-white",
					"colors": [ "#6C757D", "#ffffff" ]
				}
			],
			"gradients": [
				{
					"gradient": "linear-gradient(0deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--contrast) 100%)",
					"name": "Primary to Contrast",
					"slug": "primary-contrast"
				}
			],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#343A40",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#6C757D",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#eeeeee",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"thin": 100,
				"extra-light": 200,
				"light": 300,
				"regular": 400,
				"medium": 500,
				"semi-bold": 600,
				"bold": 700,
				"extra-bold": 800,
				"black": 900
			},
			"lineHeight": {
				"body": 1.5,
				"heading": 1.1,
				"one": 1
			},
			"spacing": {
				"outer": "30px"
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "xSmall",
					"size": "20px",
					"slug": "x-small"
				},
				{
					"name": "base",
					"size": "30px",
					"slug": "base"
				},
				{
					"name": "Small",
					"size": "clamp(30px, 4vw, 40px)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(40px, 6vw, 60px)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(50px, 8vw, 80px)",
					"slug": "large"
				},
				{
					"name": "xLarge",
					"size": "clamp(60px, 10vw, 100px)",
					"slug": "x-large"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
